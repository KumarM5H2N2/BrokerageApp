<%- include('partials/header') %>

<div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
  <h2 class="h1 display-4">Administration</h2>
</div>

<div class="card-deck row mb-3 text-center">
  <!-- user table -->
  <table class="table table-striped">
    <thead class="thead-dark">
      <tr>
        <th>id</th>
        <th>email</th>
        <th>prenom</th>
        <th>nom</th>
        <th>role</th>
        <th>created_at</th>
        <th>updated_at</th>
      </tr>
    </thead>
    <tbody>
      <% for(const user of locals.users) {%>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.prenom %></td>
        <td><%= user.nom %></td>
        <td><%= user.role %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
  <!-- end user table -->
</div>

<%- include('partials/footer') %>